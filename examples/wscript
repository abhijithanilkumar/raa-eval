# -*- Mode: python; py-indent-offset: 4; indent-tabs-mode: nil; coding: utf-8; -*-

def build(bld):
    obj = bld.create_ns3_program('raa-eval-example', ['raa-eval'])
    obj.source = 'raa-eval-example.cc'

    obj = bld.create_ns3_program('raa-eval-dense', ['internet', 'point-to-point', 'mobility', 'wifi', 'applications', 'propagation', 'raa-eval'])
    obj.source = 'raa-eval-dense.cc'
